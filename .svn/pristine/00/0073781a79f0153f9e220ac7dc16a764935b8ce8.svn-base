	/**
	* @abstract Construit une requête de suppression et l'exécute. La clause where de la requete est automatiquement construite avec l'identifiant de la fiche de stage stocké dans l'Objet courant.
	* @access public
	*/
	public  function deleteRecord(){
        $sql = "DELETE FROM ".DBFicheDevenir::$TABLE_NAME." WHERE 1";        
        $sql .= " AND ".DBFicheDevenir::$FIELD_ID_DEVENIR."=".DBConnector::getDBConnector()->processInt($this->_idDevenir);
        DBConnector::getDBConnector()->executeQuery($sql);
    }
	
	/**
	* @abstract Retourne le nom et le prénom de l'étudiant stagiaire.
	* @return String le nom et le prénom de l'étudiant sous la forme suivante : <nom> <prénom>
	* @access public
	*/
	    public function getFormSelectOptionLabel(){
        return $this->getNomEtudiant()." ".$this->getPrenomEtudiant();
    }
	
	/**
	* @abstract Retourne l'identifiant de la fiche de stage.Synonyme de la méthode getIdStage()
	* @return int Identifiant de la fiche de stage
	* @access public
	*/
    public function getFormSelectOptionValue(){
        return $this->getIdDevenir();
    }
